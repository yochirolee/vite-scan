{
	"rewrites": [{ "source": "/(.*)", "destination": "/" }],
	"headers": [
		{
			"source": "/service-worker.js",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/javascript"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=0, must-revalidate"
				}
			]
		}
	]
}
